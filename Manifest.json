{
    "manifest_version": 3,
    "name": "Brahmi Everywhere",
    "version": "1.0.0",
    "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": ["Content.js"]
     }
    ],
    "action": {
       "default_popup": "Popup.html",
       "default_title": "Brahmanandam"
    }
 }